<odoo>
    <data>
      <record id="my_video_list" model="ir.ui.view">
        <field name="name">personal.youtube.video.form</field>
        <field name="model">youtube.content</field>
        <field name="arch" type="xml">
          <form string="Youtube Video" create="False">
            <sheet>
              <div class="oe_button_box" name="button_box">
                <button class="oe_stat_button" 
                name="action_button_redirect"
                icon="fa-globe"
                type="object">
                <div class="o_field_widget o_stat_info">
                  <span class="o_stat_text">
                    Redirect
                  </span>
                </div>
                </button>
              </div>
              <div class="oe_title">
                <label for="name" />
                <h1>
                    <field name="name"/>
                </h1>
              </div>
              <notebook colspan="4">
                <page name="general_info" string="General Info">
                  <group>
                    <group>
                      <field class="video_date" readonly="1"/>
                      <field class="viewer_total" readonly="1"/>
                      <field class="like_count" readonly="1"/>
                      <field class="dislike_count" readonly="1"/>
                    </group>

                  </group>
                </page>
              </notebook>
            </sheet>
          </form>
        </field>
      </record>
    </data>
</odoo>